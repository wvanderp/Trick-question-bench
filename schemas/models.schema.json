{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Models Collection",
  "description": "Collection of OpenRouter model configurations to test",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["name", "disabled"],
    "additionalProperties": false,
    "properties": {
      "name": {
        "type": "string",
        "description": "OpenRouter model identifier (e.g., openai/gpt-4o)"
      },
      "disabled": {
        "type": "boolean",
        "description": "Whether this model is disabled and should be skipped"
      },
      "thinking": {
        "type": "string",
        "description": "Optional OpenRouter reasoning effort string"
      },
      "release_date": {
        "type": "string",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$",
        "description": "Optional model release date in YYYY-MM-DD format"
      }
    }
  }
}
