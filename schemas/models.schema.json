{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Models Collection",
  "description": "Collection of OpenRouter AI models to test",
  "type": "object",
  "required": ["models"],
  "properties": {
    "models": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "name"],
        "properties": {
          "id": {
            "type": "string",
            "description": "OpenRouter model identifier"
          },
          "name": {
            "type": "string",
            "description": "Human-readable model name"
          }
        }
      }
    }
  }
}
